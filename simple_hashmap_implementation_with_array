const arr: number[] = [1,2,4,1,2,5,3,4,2,1,5]
console.log(arr.sort())
const temp : number[] =[];
for(let i=0;i<arr.length;i++){
    
        if(arr[i] !== arr[i+1] ){
            temp.push(arr[i])
        }
    
}
console.log(temp)


//create hashmap

const myHashMap = new Map();
for(let i=0;i< arr.length;i++){
    if(!myHashMap.get(arr[i])){
        myHashMap.set(arr[i],0 )
    }
    const count = myHashMap.get(arr[i])
    myHashMap.set(arr[i],count+1 )
}

console.log(myHashMap)
